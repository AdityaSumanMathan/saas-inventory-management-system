{
  "name": "saas-inventory-management",
  "version": "1.0.0",
  "description": "SaaS Inventory Management System for Small Retail Businesses",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "start": "node server/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "db:migrate": "knex migrate:latest",
    "db:seed": "knex seed:run"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.0",
    "knex": "^2.5.1",
    "pg": "^8.11.0",
    "joi": "^17.9.2",
    "express-rate-limit": "^6.8.1",
    "express-validator": "^7.0.1",
    "dotenv": "^16.3.1",
    "socket.io": "^4.7.2",
    "stripe": "^12.14.0",
    "nodemailer": "^6.9.4",
    "multer": "^1.4.5-lts.1",
    "csv-parser": "^3.0.0",
    "json2csv": "^6.0.0-alpha.2",
    "winston": "^3.10.0",
    "compression": "^1.7.4"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.6.2",
    "supertest": "^6.3.3",
    "concurrently": "^8.2.0"
  },
  "keywords": [
    "inventory",
    "saas",
    "retail",
    "warehouse",
    "management"
  ],
